#include "holberton.h"
/**
 * main - check the code for Holberton School students.
 * Descreption - print alphabet x 10 with _putchar.
 * Return: Always 0.
 */
void print_alphabet_x10(void)
{
char i;
int a;
for (a = 0; a <= 9; a++)
{
for (i = 'a'; i <= 'z'; i++)
{
_putchar(i);
}
_putchar('\n');
}
}
